{
  "name": "smile-hub",
  "version": "1.0.0",
  "description": "Professional dental patient management system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "build": "echo \"No build step required for this project\" && exit 0",
    "postinstall": "node -e \"try { require('fs').writeFileSync('.env.example', 'DATABASE_URL=postgres://user:pass@host:5432/dbname?sslmode=require\\nSESSION_SECRET=your-secret-key-here\\nNODE_ENV=production\\nPORT=3000\\n') } catch(e) { console.error(e) }\"",
    "prepare-deploy": "node -e \"console.log('Preparing for deployment - remember to set these environment variables:\\n- DATABASE_URL: Your Neon PostgreSQL connection string\\n- SESSION_SECRET: A secure random string\\n- NODE_ENV: Set to production\\n- PORT: The port your app will run on (often set by hosting provider)');\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "bcryptjs": "^2.4.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "express-session": "^1.17.3",
    "connect-pg-simple": "^9.0.0",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "helmet": "^7.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "keywords": [
    "dental",
    "patient-management",
    "healthcare",
    "nodejs",
    "express"
  ],
  "author": "Tanuj Args",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TANUJ-args/smile-hub.git"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/TANUJ-args/smile-hub/issues"
  },
  "homepage": "https://github.com/TANUJ-args/smile-hub#readme"
}
